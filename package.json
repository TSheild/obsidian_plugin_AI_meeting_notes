{
        "name": "obsidian-ai-meeting-notes",
        "version": "0.1.0",
        "description": "Record Obsidian meetings, transcribe them with Whisper, and summarize notes with a local LLM.",
        "main": "main.js",
        "scripts": {
                "dev": "node esbuild.config.mjs",
                "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
                "test": "vitest run",
                "version": "node version-bump.mjs && git add manifest.json versions.json"
        },
        "keywords": [
                "obsidian",
                "meeting-notes",
                "whisper",
                "ollama",
                "local-ai"
        ],
        "author": "",
        "license": "MIT",
        "devDependencies": {
                "@types/node": "^20.12.7",
                "@typescript-eslint/eslint-plugin": "5.29.0",
                "@typescript-eslint/parser": "5.29.0",
                "builtin-modules": "3.3.0",
                "esbuild": "0.17.3",
                "obsidian": "latest",
                "tslib": "2.4.0",
                "typescript": "4.7.4",
                "vitest": "^1.4.0"
        }
}
